<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.rutgers.css.Rutgers2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/> -->
    <!-- <uses-permission android:name="android.permission.ACCESS_MOCK_LOCATION"/> -->

    <application
        android:name="edu.rutgers.css.Rutgers.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/default_app_icon"
        android:label="@string/app_name"
        android:theme="@style/RutgersActionBarTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <!--
     	<meta-data android:name="com.google.android.maps.v2.API_KEY"
    			   android:value="@string/google_maps_v2_key"/>
        -->

        <activity
            android:name="edu.rutgers.css.Rutgers.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="edu.rutgers.css.Rutgers.SingleFragmentActivity"
            android:label="@string/title_activity_single_fragment" >
        </activity>
        <activity
            android:name="edu.rutgers.css.Rutgers.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
    </application>

</manifest>
